        "exp.log_file" : "${BENCH_ROOT}/${exp.backend}/p100_pcie_XL270d_gen9/${exp.gpufreq}/orig/${exp.data}/${exp.dtype}/${exp.model}_${exp.replica_batch}_$(\"${exp.gpus}\".replace(\",\",\".\"))$_${exp.trial}.log",
        "exp.docker" : true,
        "exp.singularity" : false,
        "exp.phase" : "training",
