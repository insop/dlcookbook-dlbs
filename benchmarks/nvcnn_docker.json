{
    "parameters" : {
        "exp.backend" : "nvcnn",
        "exp.framework" : "tensorflow",
        "exp.framework_title" : "TensorFlow 1.7 nvcnn 18.04",
        "exp.cudnn" : "7.0",
        "exp.cuda" : "9.0",
        "exp.status" : "disabled",
        "exp.num_warmup_batches" : 100,
        "exp.num_batches" : 400,
        "exp.device_type" : "gpu",
        "exp.proj_parent_name": "",
